{
  "name": "insights",
  "version": "0.0.0",
  "author": "",
  "description": "",
  "license": "ISC",
  "main": "index.js",
  "keywords": [],
  "scripts": {
    "dev": "make up",
    "app:dev": "dotenvx run -- tsx watch ./src/infra/server.ts",
    "app:build": "dotenvx run -- tsup ./src/infra/server.ts --format esm,cjs --dts",
    "app:start": "dotenvx run -- node ./dist/index.js",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "lint:check": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "server:dev": "pnpm services:wait:database && pnpm migrate:deploy && pnpm app:dev",
    "services:wait:database": "node scripts/wait-for-postgres.mjs",
    "test": "dotenvx run -- vitest run --coverage.enabled",
    "test:unit": "dotenvx run -- vitest run --dir ./src/__tests__/unit/",
    "test:unit:watch": "dotenvx run -- vitest --dir ./src/__tests__/unit/",
    "prepare": "husky",
    "lint-staged": "lint-staged"
  },
  "engines": {
    "node": ">=22"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@dotenvx/dotenvx": "1.33.0",
    "@fastify/cors": "10.0.2",
    "@fastify/swagger": "9.4.2",
    "@fastify/swagger-ui": "5.2.1",
    "@prisma/client": "6.2.1",
    "dotenv": "16.4.7",
    "fastify": "5.2.1",
    "fastify-plugin": "5.0.1",
    "fastify-type-provider-zod": "4.0.2",
    "lodash": "4.17.21",
    "tsup": "8.3.6",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@commitlint/cli": "19.6.1",
    "@commitlint/config-conventional": "19.6.0",
    "@faker-js/faker": "9.4.0",
    "@types/lodash": "^4.17.14",
    "@types/node": "22.10.7",
    "@vitest/coverage-v8": "3.0.3",
    "@wooden-script/eslint-config": "1.0.5",
    "commitizen": "4.3.1",
    "concurrently": "9.1.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8",
    "eslint-plugin-vitest-globals": "1.5.0",
    "husky": "9.1.7",
    "lint-staged": "15.4.1",
    "prisma": "6.2.1",
    "tsx": "4.19.2",
    "typescript": "5.7.3",
    "vitest": "3.0.3"
  }
}
