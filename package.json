{
  "name": "insights",
  "version": "0.0.0",
  "author": "",
  "description": "",
  "license": "ISC",
  "main": "index.js",
  "keywords": [],
  "scripts": {
    "dev": "dotenvx run -- tsx watch src/infra/http/server.ts",
    "db:push": "dotenvx run -- drizzle-kit push --config=./src/infra/database/drizzle/index.ts",
    "db:migrate": "dotenvx run -- drizzle-kit migrate --config=./src/infra/database/drizzle/index.ts",
    "db:generate": "dotenvx run -- drizzle-kit generate --config=./src/infra/database/drizzle/index.ts",
    "db:studio": "dotenvx run -- drizzle-kit studio --config=./src/infra/database/drizzle/index.ts",
    "lint:check": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "dotenvx run -- vitest run --coverage.enabled",
    "test:unit": "dotenvx run -- vitest run --dir ./src/__tests__/unit/",
    "test:unit:watch": "dotenvx run -- vitest --dir ./src/__tests__/unit/",
    "prepare": "husky",
    "lint-staged": "lint-staged"
  },
  "engines": {
    "node": ">=22"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@dotenvx/dotenvx": "1.33.0",
    "@fastify/cors": "10.0.1",
    "@fastify/swagger": "9.4.0",
    "@fastify/swagger-ui": "5.2.0",
    "@paralleldrive/cuid2": "2.2.2",
    "@prisma/client": "6.2.1",
    "dotenv": "16.4.7",
    "fastify": "5.2.0",
    "fastify-plugin": "5.0.1",
    "fastify-type-provider-zod": "4.0.2",
    "lodash": "4.17.21",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@commitlint/cli": "19.6.1",
    "@commitlint/config-conventional": "19.6.0",
    "@faker-js/faker": "9.3.0",
    "@types/lodash": "^4.17.13",
    "@types/node": "22.10.2",
    "@vitest/coverage-v8": "2.1.8",
    "@wooden-script/eslint-config": "1.0.4",
    "commitizen": "4.3.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8",
    "eslint-plugin-vitest-globals": "1.5.0",
    "husky": "9.1.7",
    "lint-staged": "15.3.0",
    "prisma": "6.2.1",
    "tsx": "4.19.2",
    "typescript": "5.7.2",
    "vitest": "2.1.8"
  }
}
